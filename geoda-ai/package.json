    "@loaders.gl/json": "^4.1.0-alpha.4",
    "@loaders.gl/parquet": "^4.1.0-alpha.4",
    "@loaders.gl/shapefile": "^4.1.0-alpha.4",
    "@monaco-editor/react": "^4.6.0",
    "apache-arrow": "^13.0.0",
    "d3-dsv": "^3.0.1",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
