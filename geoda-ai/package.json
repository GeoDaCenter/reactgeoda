{
  "name": "geoda-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env BASE_PATH='' next dev",
    "build-local": "cross-env NODE_ENV=production BASE_PATH='' next build",
    "get-kepler": "cd ../.. && git clone https://github.com/GeoDaCenter/kepler.gl.git --branch=xli/reactgeoda csds_kepler",
    "build-kepler": "yarn set version 1.22.19 && yarn --cwd ../../csds_kepler install",
    "build-netlify": "yarn get-kepler && yarn build-kepler && yarn build-local",
    "start": "next start",
    "prettier": "prettier --ignore-path ./.prettierignore . '**/*.{js,jsx,ts,tsx}' --check",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-intl": "^6.5.5",
    "react-responsive-modal": "^6.4.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/postcss-reporter": "^7",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18",
    "@types/react-grid-layout": "^1.3.5",
    "@types/redux-logger": "^3.0.12",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "postcss-reporter": "^7.0.5",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "react-redux": "^9.0.4",
    "react-virtualized-auto-sizer": "^1.0.20",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react-palm": "3.3.8"
  }
}
